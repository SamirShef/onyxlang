import Std.Math;

mod Module {
    pub var c: i32;

    pub fun Add(a: i32, b: i32): i32 {
        self.c += 10;
        return parent.add(a, b);    // accessing member from parent module
    }

    fun add(): i32 {
        return self.Add(2, 3);      // accessing member from current module
    }
}

fun main(): i32 {
    var x: i32 = Std.Math.Abs(-1);
    echo x; echo '\n';
    echo Module.c + 10; echo '\n';
    return 0;
}

fun add(a: i32, b: i32): i32 {
    return a + b;
}